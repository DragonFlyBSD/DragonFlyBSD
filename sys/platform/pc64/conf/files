	dependency	"$S/dev/raid/hptrr/x86_64-elf.hptrr_lib.o.uu"	\
	compile-with	"uudecode < $S/dev/raid/hptrr/x86_64-elf.hptrr_lib.o.uu" \
	no-implicit-rule
