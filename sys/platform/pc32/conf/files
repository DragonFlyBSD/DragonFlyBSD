	dependency	"$S/dev/raid/hpt27xx/i386-elf.hpt27xx_lib.o.uu"	\
	compile-with	"uudecode < $S/dev/raid/hpt27xx/i386-elf.hpt27xx_lib.o.uu" \
	no-implicit-rule
#
