# $FreeBSD: src/tools/regression/bin/sh/expansion/cmdsubst13.0,v 1.1 2012/01/14 23:10:18 jilles Exp $

x=1 y=2
[ "$(
	case $((x+=1)) in
	($((y+=1)))	echo bad1 ;;
	($((y-1)))	echo $x.$y ;;
	($((y=2)))	echo bad2 ;;
	(*)		echo bad3 ;;
	esac
)" = "2.3" ] || echo "Error at $LINENO"
[ "$x.$y" = "1.2" ] || echo "Error at $LINENO"
